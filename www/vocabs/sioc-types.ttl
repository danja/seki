<http://rdfs.org/sioc/types#WikiArticle>
    a owl:Class ;
    rdfs:comment "Describes a wiki article."@en ;
    rdfs:isDefinedBy <http://rdfs.org/sioc/types#> ;
    rdfs:label "Wiki Article"@en ;
    rdfs:seeAlso <http://rdfs.org/sioc/types#Wiki> ;
